{
  "name": "fast-app-template",
  "version": "0.1.0",
  "description": "fast-app-template",
  "scripts": {
    "serve": "fast-app-service serve",
    "build": "fast-app-service build",
    "dll": "fast-app-service dll",
    "lint": "eslint --ext .vue,.js ./src --cache",
    "lint:fix": "eslint --fix --ext .vue,.js ./src --cache",
    "wifiStart": "fast-app wifiStart",
    "wifiStop": "fast-app wifiStop",
    "wifiSync": "fast-app wifiSync -r ./dist -u false",
    "wifiSyncAll": "fast-app wifiSync -r ./dist -u true",
    "wifiInfo": "fast-app wifiInfo",
    "wifiLog": "fast-app wifiLog "
  },
  "author": "tangfei",
  "license": "UNLICENSED",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "globals": {
      "api": true,
      "$api": true,
      "App": true,
      "console": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module",
      "ecmaVersion": 2018,
      "ecmaFeatures": {
        "globalReturn": false,
        "impliedStrict": false,
        "jsx": false
      },
      "allowImportExportEverywhere": false,
      "codeFrame": true
    },
    "rules": {
      "no-debugger": 1,
      "semi": [
        "error",
        "always"
      ],
      "no-console": "off",
      "arrow-parens": 0
    }
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions",
    "Android >= 3.2"
  ],
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "fast-app-service": "^0.1.0"
  }
}
